#include <iostream>
using namespace std;

int main() {
    int a, b, c;
    cout << "Nhap 3 so a, b, c: ";
    cin >> a >> b >> c;

    int temp; // biến phụ

    // Đảm bảo a <= b
    if (a > b) {
        temp = a;
        a = b;
        b = temp;
    }

    // Đảm bảo a <= c
    if (a > c) {
        temp = a;
        a = c;
        c = temp;
    }

    // Đảm bảo b <= c
    if (b > c) {
        temp = b;
        b = c;
        c = temp;
    }

    cout << "Thu tu tang dan: " << a << " " << b << " " << c << endl;
    return 0;
}
